{
  "manifest_version": 2,

  "name": "Undead",
  "description": "Modifies AoPS",
  "version": "1.1",

  "icons": { "16": "daisy_16.png",
           "48": "daisy_48.png",
          "128": "daisy_128.png" },  

  "content_scripts": [
    {
      "matches": ["https://artofproblemsolving.com/*"],
      "js": ["jquery-2.1.4.min.js", "undead.js"],
      "run_at": "document_end"
    }
  ]
}